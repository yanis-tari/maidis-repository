    <bpmn2:scriptTask id="_3BDD4B1B-D604-45AE-A146-9DE3B59A2DE6" drools:selectable="true" name="init WM" scriptFormat="http://www.java.com/java">
      <bpmn2:incoming>_2F16B9CC-5410-4B10-87EC-E7C4684DBA42</bpmn2:incoming>
      <bpmn2:outgoing>_691CE5E3-67F2-46FD-A1C8-B408A0F1AB3B</bpmn2:outgoing>
      <bpmn2:script><![CDATA[if( id != null && birthdate != null){ 
   System.out.println("Insert in WM");
   patient = new com.mds.myhealth.Patient();
   java.util.List<com.mds.myhealth.SimpleExam> exams = new java.util.ArrayList<com.mds.myhealth.SimpleExam>();  
  
  
  if(systolicBP != null){ 
  com.mds.myhealth.SimpleExam sysBP = new com.mds.myhealth.SimpleExam();
  sysBP.setName("systolicBP");
  sysBP.setIntValue(systolicBP);
  sysBP.setDate(new java.util.Date());
  exams.add(sysBP);
  
  }
  
  
  if(diastolicBP != null){ 
  com.mds.myhealth.SimpleExam diasBP = new com.mds.myhealth.SimpleExam();
  diasBP.setName("diastolicBP");
  diasBP.setIntValue(diastolicBP);
  diasBP.setDate(new java.util.Date());
  exams.add(diasBP);
  }
  
  
  if(pulse != null){ 
  com.mds.myhealth.SimpleExam pulseobj = new com.mds.myhealth.SimpleExam();
  pulseobj.setName("pulse");
  pulseobj.setIntValue(pulse);
  pulseobj.setDate(new java.util.Date());
  exams.add(pulseobj);
  }
  
  patient.setExams(exams);
  patient.setId(id);   
  
 java.text.SimpleDateFormat formatter = new java.text.SimpleDateFormat("MM/dd/yyyy");
		try {	 
			java.util.Date date = formatter.parse(birthdate);
            patient.setBirthDate(date);        
			System.out.println(formatter.format(date)); 			
		} catch (java.text.ParseException e) {
			e.printStackTrace();
		}finally{		
		kcontext.getKnowledgeRuntime().insert(patient);			
		}
kcontext.getKnowledgeRuntime().insert(patient);                                          
System.out.println("Insert in WM success");    
}]]></bpmn2:script>
